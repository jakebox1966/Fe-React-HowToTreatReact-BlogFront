{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "react-app",
    "prettier"
  ],

  "parser": "@babel/eslint-parser",

  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    //console.log 사용 시 경고
    "no-unused-vars": "warn"
  }
}